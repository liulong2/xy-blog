<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xybbz.modules.log.mapper.LogErrDAO">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xybbz.logentity.LogLocalErr">
        <result column="ram_addr" property="ramAddr" />
        <result column="url" property="url" />
        <result column="error_message" property="errorMessage" />
        <result column="error_file" property="errorFile" />
        <result column="line_number" property="lineNumber" />
        <result column="error_name" property="errorName" />
        <result column="log_id" property="logId" />
    </resultMap>



    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ram_addr, url, error_message, error_file, line_number, error_name, log_id
    </sql>


</mapper>
